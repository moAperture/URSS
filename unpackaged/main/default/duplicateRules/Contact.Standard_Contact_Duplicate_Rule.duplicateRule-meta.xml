    <actionOnUpdate>Block</actionOnUpdate>
    <alertText>You&#39;re creating a duplicate record with same passport number. We recommend you use an existing record instead.</alertText>
    <description>Duplicate Rule for Contacts using the Standard Contact Matching Rule</description>
    <duplicateRuleFilter xsi:nil="true" />
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>Contact</matchRuleSObjectType>
        <matchingRule>Passport_Number_Match</matchingRule>
